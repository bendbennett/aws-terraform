[
  {
    "name": "mongo",
    "image": "bendbennett/mongo-seeded:v0.3.0",
    "cpu": 1024,
    "memory": 993,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 27017,
        "hostPort": 27017
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "eu-west-1",
        "awslogs-group": "symfony-api-swagger-jwt",
        "awslogs-stream-prefix": "mongo"
      }
    }
  }
]
